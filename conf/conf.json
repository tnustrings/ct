{
    "proglang": [
	    {
		"name": "python",
		"ext": ["py"],
		"fncre": "def\\s+(?P<name>[\\w\\d_]+)\\s*\\(.*\\).*:",
                "cmtopen": "\"\"\"",
		"cmtclose": "\"\"\"",
		"cmtindent": "    ",
		"fnccmt": "after"
	    },
	    {
		"name": "go",
		"ext": ["go"],
		"fncre": "func\\s+(?P<name>[\\w\\d_]+).*\\(.*\\).*{", 
		"cmtline": "//",
		"fnccmt": "before"
	    },
	    {
		"name": "javascript",
		"ext": ["js", "ts"],
		"fncre": "(function)?\\s+(?P<name>[\\w\\d_]+).*\\(.*\\)\\s*{", 
		"cmtline": "//",
		"fnccmt": "before"
	    }
    ]
}
